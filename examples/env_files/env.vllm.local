# Example .env file for local vLLM server
# Copy this to .env in the project root

# LLM Provider
LLM_PROVIDER=vllm

# vLLM Configuration
VLLM_ENDPOINT=http://localhost:8000
VLLM_MODEL_NAME=Qwen/Qwen3-32B-FP8
VLLM_API_KEY=dummy_key

# General LLM Settings
LLM_TIMEOUT=1200
LLM_MAX_TOKENS=8000
LLM_TEMPERATURE=0.1

# Project Configuration
DEFAULT_OUTPUT_DIR=./output
DEFAULT_CPP_STANDARD=C++17
DEFAULT_MAX_TURNS=2
DEFAULT_TARGET_QUALITY=7.0
DEFAULT_INCLUDE_TESTS=true

# Logging Configuration
LOG_LEVEL=INFO
ENABLE_CONSOLE_LOGGING=true

# Development Configuration
DEV_MODE=false
